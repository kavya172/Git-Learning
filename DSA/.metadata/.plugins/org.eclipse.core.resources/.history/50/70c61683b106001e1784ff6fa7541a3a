public class linearSearch {
	
	public static void main(String args[]) {
		int linearSearch[]= {1,2,11,34,56,76,43,21,211,12,1234};
		Integer searchElement = 12;
		
		linearSearchForGivenList(linearSearch,searchElement);
	}
	private static void linearSearchForGivenList(int linearSearch[], Integer searchElement) {
		
		for(int i=0; i<linearSearch.length;i++) {
		   if(searchElement.equals(linearSearch[i])) {
			   System.out.println("Found at index : "+ i );
		   }
		}
	}
}
